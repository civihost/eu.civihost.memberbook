<?xml version="1.0"?>
<extension key="eu.civihost.memberbook" type="module">
  <file>memberbook</file>
  <name>Member Book</name>
  <description>Extension to print the Member Book according to the Italian law for Associations an Co-operatives</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Samuele Masetto (CiviHOST - Perora Srl SB)</author>
    <email>info@civihost.it</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/civihost/eu.civihost.memberbook</url>
    <url desc="Documentation">https://github.com/civihost/eu.civihost.memberbook</url>
    <url desc="Support">https://github.com/civihost/eu.civihost.memberbook/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-06-07</releaseDate>
  <version>0.3</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.77</ver>
  </compatibility>
  <comments>This extension creates two Member report templates to print the Member Book according to the Italian law for Associations an Co-operatives</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Memberbook</namespace>
    <format>23.02.1</format>
    <angularModule>crmMemberbook</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>setting-admin@1.0.1</mixin>
  </mixins>
</extension>
